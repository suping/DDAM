	<!-- requires="trace" policySets="traceImplPolicySet traceBindingPolicySet"-->
		
<composite	xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		xmlns:tuscany="http://tuscany.apache.org/xmlns/sca/1.1"
		xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		targetNamespace="http://store"
		name="proc-node"
		 >
        
		<component name="ProcComponent">
			<implementation.java class="services.ProcServiceImpl" 
				/>
			
			<service name="ProcService" requires="trace" policySets="traceBindingPolicySet">
				<tuscany:binding.jsonrpc uri="http://114.212.84.46:8083/ProcComponent/ProcService" />
			</service>
			
			<reference name="db" >
				<tuscany:binding.jsonrpc uri="http://114.212.84.46:8080/DBComponent/DBService" />
			</reference>
			<reference name="verify" >
				<tuscany:binding.jsonrpc uri="http://114.212.84.46:8082/VerificationComponent/VerificationService" />
			</reference>		
		</component>  

</composite>
